{
  "name": "job-recommender-api",
  "version": "1.0.0",
  "description": "Express Typescript NodeJS Starter with Docker",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect=8181 --respawn --transpile-only src/index.ts",
    "start": "node ./dist/src/index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format:check": "prettier -c \"**/*.{js,ts,json}\"",
    "format": "prettier -w \"**/*.{js,ts,json}\"",
    "lint": "eslint --fix ./src/*",
    "prepare": "husky install",
    "pre-commit": "npm run typecheck && lint-staged && jest && npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --fix",
    "**/*.{js,ts,json}": "prettier --write"
  },
  "keywords": [],
  "author": "binoy638",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "bcrypt": "^5.0.1",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.4",
    "winston": "^3.6.0",
    "zod": "^3.17.3"
  }
}
